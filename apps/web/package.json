{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "unirefund": "env-cmd -f env/.env.unirefund next dev --port 3000",
    "upwithcrowd": "env-cmd -f env/.env.upwithcrowd next dev --port 3000",
    "build": "next build",
    "build:unirefund": "env-cmd -f env/.env.unirefund next build",
    "build:upwithcrowd": "env-cmd -f env/.env.upwithcrowd next build",
    "start": "next start",
    "start:unirefund": "env-cmd -f env/.env.unirefund next start",
    "start:upwithcrowd": "env-cmd -f env/.env.upwithcrowd next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:report": "next lint --quiet  > lint-output.txt 2>&1",
    "type-check": "tsc --noEmit",
    "type-check:report": "tsc --noEmit > type-check-output.txt 2>&1",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "format": "prettier --write . --plugin=prettier-plugin-tailwindcss",
    "precommit": "prettier --write . && next lint --fix && tsc --noEmit "
  },
  "lint-staged": {
    "*": [
      "prettier --write",
      "next lint --fix",
      "tsc --noEmit"
    ]
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@ayasofyazilim/saas": "^0.0.33",
    "@formatjs/intl-localematcher": "^0.5.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@repo/ayasofyazilim-ui": "workspace:^",
    "@repo/ui": "workspace:*",
    "@tanstack/react-table": "8.16.0",
    "@types/google-libphonenumber": "^7.4.30",
    "@vercel/blob": "0.22.1",
    "cal-sans": "^1.0.1",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "geist": "^1.3.0",
    "google-libphonenumber": "^3.2.37",
    "lucide-react": "0.363.0",
    "mini-svg-data-uri": "^1.4.4",
    "negotiator": "0.6.3",
    "next": "14.1.1",
    "next-auth": "5.0.0-beta.16",
    "next-view-transitions": "0.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.1.1",
    "@playwright/test": "^1.44.1",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/negotiator": "0.6.3",
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "autoprefixer": "10.4.18",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
