{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "resources": "node -r dotenv/config get-resources.js && prettier --write \"src/language-data/resources.ts\" --log-level=silent",
    "dev": "pnpm resources && next dev",
    "build": "pnpm resources && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:report": "next lint --quiet  > lint-output.txt 2>&1",
    "type-check": "tsc --noEmit",
    "type-check:report": "tsc --noEmit > type-check-output.txt 2>&1",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "format": "prettier --write . --plugin=prettier-plugin-tailwindcss",
    "precommit": "prettier --write . && next lint --fix && tsc --noEmit ",
    "lint-staged": "npx lint-staged --config .lintstagedrc.js"
  },
  "lint-staged": {
    "*": [
      "prettier --write . --plugin=prettier-plugin-tailwindcss",
      "next lint --fix",
      "tsc --noEmit"
    ]
  },
  "dependencies": {
    "@auth/core": "^0.30.0",
    "@ayasofyazilim/core-saas": "latest",
    "@ayasofyazilim/saas": "0.0.165",
    "@ayasofyazilim/unirefund-saas-dev": "^0.0.4",
    "@clickhouse/client": "^1.11.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@repo/actions": "workspace:*",
    "@repo/ayasofyazilim-ui": "workspace:^",
    "@repo/saas": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/utils": "workspace:*",
    "@superset-ui/embedded-sdk": "^0.1.3",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/react-table": "8.16.0",
    "@types/google-libphonenumber": "^7.4.30",
    "card-validator": "^10.0.3",
    "dotenv": "^16.4.5",
    "env-cmd": "^10.1.0",
    "eslint-plugin-react-require-testid": "^1.0.4",
    "geist": "1.3.0",
    "google-libphonenumber": "^3.2.37",
    "lodash": "^4.17.21",
    "lucide-react": "0.545.0",
    "mini-svg-data-uri": "^1.4.4",
    "next": "14.2.29",
    "next-auth": "5.0.0-beta.16",
    "ogl": "^1.0.11",
    "react": "18.3.1",
    "react-aria": "^3.37.0",
    "react-dom": "18.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.1.1",
    "@playwright/test": "^1.55.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "20.11.24",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/lodash": "^4.17.20",
    "autoprefixer": "10.4.18",
    "lint-staged": "15.2.2",
    "postcss": "8.4.35",
    "tailwindcss": "3.4.1",
    "typescript": "5.3.3"
  }
}
